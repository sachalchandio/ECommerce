<% provide(:title, brandEditForm) %>
<div class = 'brandNewForm'>
  <div class="container">
   <div class="row">
       <table class="table table-striped">
  
        <tbody>
  
          <tr> 
          <%= simple_form_for @brand do |f| %>
          <%= f.input :name %>
          <%= f.button :submit, class:"btn btn-primary" %>
          <%= f.association :sub_departments, :as => :check_boxes %>
          <% end %> </tr>
            
        </tbody>
      </table>
    </div>
  </div>
</div>